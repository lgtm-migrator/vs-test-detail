{
    "manifestVersion": 1,
    "id": "vss-test-details",
    "publisher": "AnthonyShaw",
    "version": "1.0.99",
    "name": "Test Details",
    "description": "An extension for seeing detailed test data in the test results UI.",
    "categories": ["Azure Pipelines"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": ["vso.test"],
    "contributions": [
        {
            "id": "test-detail-item",
            "type": "ms.vss-web.tab",
            "targets": [
                "ms.vss-test-web.test-result-details-tab-items"
            ],
            "description": "Extension to show nunit test data",
            "properties": {
                "name": "Test Details",
                "uri": "test-detail.html",
                "order": 107
            }
        }
    ],
    "files": [
        {
            "path": "test-detail.html",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ],
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "license": {
        "path": "LICENSE"
    },
    "links": {
        "source": {
            "uri": "https://github.com/tonybaloney/vs-test-detail"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/tonybaloney/vs-test-detail"
    },
    "badges": [
        {
            "href": "https://dev.azure.com/AnthonyShaw/vs-test-detail/_build/latest?definitionId=6&branchName=master",
            "uri": "https://dev.azure.com/AnthonyShaw/vs-test-detail/_apis/build/status/tonybaloney.vs-test-detail?branchName=master",
            "description": "Azure Pipelines Status"
        }
    ],
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/screenshot.png"
        },
        {
            "path": "images/screenshot2.png"
        },
        {
            "path": "images/screenshot3.png"
        }
    ],
    "icons": {
        "default": "images/logo.png"
    }
}